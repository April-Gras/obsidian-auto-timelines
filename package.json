		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"esbuild-plugin-vue3": "^0.3.2",
		"obsidian": "latest",
		"sass": "^1.62.1",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	},
	"dependencies": {
		"vue": "^3.2.47",
		"vue-i18n": "^9.2.2"
