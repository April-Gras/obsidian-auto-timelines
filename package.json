{
	"name": "obsidian-auto-timelines",
	"version": "0.13.4",
	"description": "This Obsidian (https://obsidian.md) plugin aims to simplify creating and maintaining timelines in different a project",
	"main": "main.js",
	"type": "module",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"test": "vitest",
		"test-ui": "vitest --ui --coverage",
		"coverage": "vitest --coverage",
		"prepare-vitest": "node prepare-vitest.mjs"
	},
	"keywords": [],
	"author": "April",
	"license": "MIT",
	"devDependencies": {
		"@types/lodash.clonedeep": "^4.5.9",
		"@types/lodash.isequal": "^4.5.8",
		"@types/node": "^18.19.41",
		"@typescript-eslint/eslint-plugin": "7.8.0",
		"@typescript-eslint/parser": "7.8.0",
		"@vitejs/plugin-vue": "^5.0.5",
		"@vitest/ui": "^2.0.3",
		"builtin-modules": "4.0.0",
		"esbuild": "0.23.0",
		"esbuild-plugin-vue3": "^0.4.2",
		"eslint": "^8.57.0",
		"eslint-plugin-jsdoc": "^48.7.0",
		"happy-dom": "^14.7.1",
		"lodash.clonedeep": "^4.5.0",
		"obsidian": "1.6.6",
		"sass": "^1.77.8",
		"ts-essentials": "^10.0.1",
		"tslib": "2.6.3",
		"typescript": "5.5.3",
		"vite-tsconfig-paths": "^4.3.2",
		"vitest": "^2.0.3",
		"vitest-mock-extended": "^1.3.2"
	},
	"dependencies": {
		"@vitest/coverage-v8": "^2.0.3",
		"lodash.isequal": "^4.5.0",
		"vue": "^3.4.32",
		"vue-collapsed": "^1.3.3",
		"vue-feather": "^2.0.0",
		"vue-i18n": "^9.13.1",
		"yaml": "^2.4.5"
	}
