{
	"name": "obsidian-auto-timelines",
	"version": "0.8.1",
	"description": "This Obsidian (https://obsidian.md) plugin aims to simplify creating and maintaining timelines in different a project",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"test": "vitest",
		"test-ui": "vitest --ui --coverage",
		"coverage": "vitest --coverage",
		"prepare-vitest": "node prepare-vitest.mjs"
	},
	"keywords": [],
	"author": "April",
	"license": "MIT",
	"devDependencies": {
		"@types/lodash.clonedeep": "^4.5.7",
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "6.5.0",
		"@typescript-eslint/parser": "6.5.0",
		"@vitejs/plugin-vue": "^4.3.4",
		"@vitest/coverage-istanbul": "^0.34.3",
		"@vitest/ui": "^0.34.3",
		"@vue/test-utils": "^2.4.1",
		"builtin-modules": "3.3.0",
		"esbuild": "0.19.2",
		"esbuild-plugin-vue3": "^0.3.2",
		"eslint": "^8.48.0",
		"eslint-plugin-jsdoc": "^46.5.1",
		"happy-dom": "^10.11.2",
		"lodash.clonedeep": "^4.5.0",
		"obsidian": "latest",
		"sass": "^1.66.1",
		"ts-essentials": "^9.4.0",
		"tslib": "2.6.2",
		"typescript": "5.2.2",
		"vite-tsconfig-paths": "^4.2.0",
		"vitest": "^0.34.3",
		"vitest-mock-extended": "^1.2.0"
	},
	"dependencies": {
		"vue": "^3.3.4",
		"vue-collapsed": "^1.2.7",
		"vue-i18n": "^9.2.2",
		"yaml": "^2.3.2"
	}
}
